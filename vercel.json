{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/product/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/content/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/health/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
